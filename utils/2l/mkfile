BIN=$ROOT/$OBJDIR/bin
<$ROOT/mkfiles/mkone-$SHELLTYPE

CFLAGS=	$CFLAGS -I ../include

enam.$O:	../2c/enam.c
	$CC $CFLAGS ../2c/enam.c
