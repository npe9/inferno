<../../mkconfig

TARG=2l
OFILES=\
	asm.$O\
	obj.$O\
	optab.$O\
	pass.$O\
	span.$O\
	list.$O\
	enam.$O\
	$TARGMODEL.$O\

HFILES=\
	l.h\
	../2c/2.out.h\
	../include/ar.h\

